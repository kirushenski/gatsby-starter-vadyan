{
  "name": "gatsby-starter-vadyan",
  "version": "1.0.1",
  "license": "MIT",
  "author": "p1t1ch <belobeev.kirill@gmail.com>",
  "scripts": {
    "dev": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "start": "gatsby serve -H 0.0.0.0 -p 8000",
    "storybook": "start-storybook -s public -p 8080 --ci",
    "storybook:build": "build-storybook -s public -o docs",
    "storybook:start": "serve docs -l 8080",
    "test": "is-ci test:coverage test:watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "is-ci test:e2e:run test:e2e:open",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "server-test 8000 cy:run",
    "test:e2e:open": "start-server-and-test dev 8000 cy:open",
    "coverage": "open ./coverage/lcov-report/index.html",
    "stats": "open ./public/report.html",
    "lint": "eslint . --ext .tsx,.ts,.mdx,.js --fix",
    "format": "prettier  . --write --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn format && yarn type-check && yarn test:coverage && yarn test:e2e:run",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn type-check"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,mdx,js}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "public",
    "docs",
    "coverage",
    "!.storybook"
  ],
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ],
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@svgr/webpack": "^5.4.0",
    "cross-fetch": "^3.0.5",
    "emotion-theming": "^10.0.27",
    "focus-visible": "^5.1.0",
    "gatsby": "2.24.6",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-manifest": "^2.4.22",
    "gatsby-plugin-offline": "^3.2.22",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.25",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.11",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.12",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/react": "next",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/testing-library__jest-dom": "^5.9.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^26.2.2",
    "babel-preset-gatsby": "^0.5.5",
    "cypress": "^4.12.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-dom": "^3.1.6",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mdx": "^1.8.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.4.1",
    "husky": "^4.2.5",
    "is-ci-cli": "2.1.2",
    "jest": "^26.2.2",
    "jest-emotion": "^10.0.32",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.3",
    "typescript": "^3.9.7"
  },
  "resolutions": {
    "babel-plugin-remove-graphql-queries": "2.9.14",
    "jsdom": "16.3.0"
  },
  "engines": {
    "node": ">= 10.14.2"
  }
}
