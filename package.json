{
  "name": "gatsby-starter-vadyan",
  "version": "2.1.0",
  "license": "MIT",
  "author": "p1t1ch <belobeev.kirill@gmail.com>",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0 -p 8000",
    "storybook": "start-storybook -s public",
    "storybook:build": "build-storybook -s public -o docs",
    "storybook:serve": "serve docs -l 8080",
    "test": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "coverage": "open ./coverage/lcov-report/index.html",
    "stats": "open ./public/report.html",
    "lint": "eslint . --ext .tsx,.ts,.js --fix",
    "format": "prettier . --write --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format && npm run type-check && npm run test:coverage",
    "clean": "gatsby clean",
    "setup": "npm i --legacy-peer-deps && npm run validate",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "public",
    "coverage",
    "docs",
    "!.storybook"
  ],
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ],
  "dependencies": {
    "@fontsource/manrope": "^4.2.2",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.2.5",
    "focus-visible": "^5.2.0",
    "gatsby": "^3.3.0",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-postcss": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "postcss": "^8.2.10",
    "postcss-focus-visible": "^5.0.0",
    "postcss-import": "^14.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss": "2.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-a11y": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.3",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.3.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jest-dom": "^3.8.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.0.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.4",
    "webpack": "5.28.0",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "engines": {
    "node": ">= 12.13.0"
  }
}
