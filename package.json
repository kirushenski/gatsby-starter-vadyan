{
  "name": "gatsby-starter-vadyan",
  "version": "1.1.0",
  "license": "MIT",
  "author": "p1t1ch <belobeev.kirill@gmail.com>",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0 -p 8000",
    "storybook": "start-storybook -s public -p 8080 --ci",
    "storybook:build": "build-storybook -s public -o docs",
    "storybook:serve": "serve docs -l 8080",
    "test": "is-ci test:coverage test:watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "coverage": "open ./coverage/lcov-report/index.html",
    "stats": "open ./public/report.html",
    "lint": "eslint . --ext .tsx,.ts,.js --fix",
    "format": "prettier . --write --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format && npm run type-check && npm run test:coverage",
    "clean": "gatsby clean",
    "setup": "npm i && npm run validate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "public",
    "docs",
    "coverage",
    "!.storybook"
  ],
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ],
  "dependencies": {
    "@fontsource/manrope": "^4.2.2",
    "@svgr/webpack": "^5.4.0",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "focus-visible": "^5.2.0",
    "gatsby": "^2.24.75",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-offline": "^3.2.31",
    "gatsby-plugin-postcss": "^2.3.13",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-sharp": "^2.5.17",
    "postcss": "^7.0.35",
    "postcss-focus-visible": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/testing-library__jest-dom": "^5.9.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-jest": "^26.5.2",
    "babel-preset-gatsby": "^0.5.14",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.1",
    "husky": "^4.3.0",
    "is-ci-cli": "2.1.2",
    "jest": "^26.5.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">= 10.14.2"
  }
}
