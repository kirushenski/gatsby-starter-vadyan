{
  "name": "gatsby-starter",
  "version": "0.1.0",
  "description": "Gatsby starter for personal projects",
  "keywords": [
    "gatsby"
  ],
  "repository": "https://github.com/p1t1ch/gatsby-starter",
  "author": "p1t1ch <belobeev.kirill@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .tsx,.ts,.mdx,.js --ignore-path .gitignore --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run build",
    "setup": "npm i && npm run validate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,mdx,js}": [
      "eslint . --ext .tsx,.ts,.mdx,.js --ignore-path .gitignore --fix"
    ]
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-testing-library": "^3.3.1",
    "gatsby": "^2.23.22",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-plugin-emotion": "^4.3.9",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "typescript": "^3.9.6"
  }
}
